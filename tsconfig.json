{
  "compilerOptions": {
    "baseUrl": ".",
    "typeRoots": ["node_modules/@types", ".types"],
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@models/*": ["src/models/*"],
      "@api/*": ["src/api/*"],
      "@navigation/*": ["src/navigation/*"],
      "@screens/*": ["src/screens/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
